					<script type="text/javascript">
						document.onkeydown = function (e) {
	            		var theEvent = window.event || e;
	            		var code = theEvent.keyCode || theEvent.which;
	            			if (code == 13) {
	                			jumpPage();
	            				}
	        			}
						function jumpPage(){
			   			var reg = new RegExp("^[0-9]*$");
		       			var inputPageNo = $(".jQ-pagination-input").val();
		       			if(inputPageNo == '') {
		       				} else if(!reg.test(inputPageNo)){
		           		alert("页码输入错误!");
		           		$(".jQ-pagination-input").val("");
		       				} else {
		           		turnOverPage(inputPageNo);
		       				}	    
						}
		  				function turnOverPage(no){
		    				var qForm=document.qPagerForm;
		    				if(no>44){no=44;}
		    				if(no<1){no=1;}
		    				qForm.pageNo.value=no;
		    				qForm.action="search"; 
		    				setTimeout(function(){ 
		    					qForm.submit();
		    				},0);
		    				return false;
		  				}
					</script>
